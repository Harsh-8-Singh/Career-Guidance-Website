                        qpa[12][1]="neutral flame";
                        qpa[12][2]="oxidizing flames";
                        qpa[12][3]="reducing flame";
                        qpa[12][4]="carburising flame";

                        qpa[13][0]="Upsetting is the term used in one of the following operations";
                        qpa[13][1]="forging";
                        qpa[13][2]="drilling";
                        qpa[13][3]="turning";
                        qpa[13][4]="casting";

